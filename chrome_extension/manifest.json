{
  "name": "QuickExchange - File Swapper",
  "version": "1.0",
  "description": "Useful extension that provides the same functionality as the QuickExchange webapp",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "favicon/favicon-16x16.png",
    "default_popup": "popup.html",
    "default_title": "QuickExchange"
  },
  "icons": { 
    "16": "favicon/favicon-16x16.png",
    "48": "favicon/favicon-32x32.png",
   "128": "favicon/android-chrome-192x192.png" 
  },
  "permissions": [
    "storage"
  ]
}
