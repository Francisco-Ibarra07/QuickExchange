{
  "name": "Quick Exchange Extension",
  "version": "1.0",
  "description": "Useful extension that provides the same functionality as the QuickExchange webapp",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "favicon/favicon-16x16.png",
    "default_popup": "popup.html",
    "default_title": "QuickExchange"
  }
}
